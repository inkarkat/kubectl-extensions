#!/bin/bash
set -o pipefail

typeset -a reldate=()
type -t reldate >/dev/null && reldate=(reldate --date-pattern '\b(?<year>\d\d\d\d)-(?<month>\d\d)-(?<day>\d\d)T(?<hours>\d\d):(?<minutes>\d\d):(?<seconds>\d\d)(?<timezone>Z)\b' --prefer-past)

exec kubectl-podcontainerages "$@" "${reldate:+|}" '"${reldate[@]}"'
